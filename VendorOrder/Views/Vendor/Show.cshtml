  <body>
      @using VendorOrder.Models;

    <h3>Here are all the orders in this Vendor:</h3>
    <ol>
    @foreach (Order order in @Model["order"])
    {
      test show in vendor
      <li><a href='/vendors/@Model["vendor"].Id/orders/@order.Id'>@order.OrName</a></li>
    }
    </ol>
    <p><a href='/vendors/@Model["vendor"].Id/orders/new'>Add a new item</a></p>
    <p><a href='/vendors'>Return to vendors list</a></p>
    <p><a href='/'>Return to Main Page</a></p> 
  </body>
</html>